package geometri;

/**
 * @author Anton Solback, Rasmus Davidsson. Group nr 40.
 */

import java.awt.Color;
import java.awt.Graphics;

public class Square extends Shapes {

	private int side;



	/**
	 * A constructor for Square
	 * @param x Distance right from origo
	 * @param y Distance down from origo
	 * @param side Both width and height of the square
	 * @param c The color of the square
	 * @throws IllegalPositionException if the coordinates are invalid
	 * 
	 */
	public Square(int x, int y, int side, Color c) throws IllegalPositionException{}


	/**
	 * A constructor for Square
	 * @param f A GeometricalForm object
	 * @param side Both width and height of the square
	 * @param c The color of the square
	 * 
	 */
	public Square(GeometricalForm f, int side, Color c){}

	
}
