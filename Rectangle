package geometri;

/**
 * @author Anton Solback, Rasmus Davidsson. Group nr 40.
 */

import java.awt.Color;
import java.awt.Graphics;

public class Rectangle extends Shapes{

	private int width;
	private int height;
	
	/**
	 * 
	 * @param x Distance right from origo.
	 * @param y Distance down fron origo.
	 * @param width The width of the Rectangle
	 * @param height The height of the Rectangle
	 * @param c The color of the Rectangle
	 * @throws IllegalPositionException If the coordinations are invalid
	 */
	
	public Rectangle(int x, int y, int width, int height, Color c) throws IllegalPositionException{}
	
	/**
	 * 
	 * @param f A GeometricalForm object
	 * @param width The width of the Rectangle
	 * @param height The height of the Rectangle
	 * @param c The color of the Rectangle
	 */
	public Rectangle(GeometricalForm f, int width, int height, Color c){}
	
}
